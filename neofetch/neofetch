#!/bin/bash
# Neofetch: A command-line system information tool written in bash 3.2+.
# https://github.com/dylanaraps/neofetch
# add the following ASCII art to /usr/bin/neofetch
	"DampDigits"*)
		set_colors 6 4 5
            read -rd '' ascii_data <<'EOF'
${c1}       ....                                  
    :+++++++++                               
   =++++++++++++                             
  :+++++++++++++++:                          
  =+++++++++${c2}000000000${c3}::::::::::::::::::..${c1}    
  =+++++++${c2}00000000000000${c3}::::::::::::::::::.${c1}  
  =++++++${c2}00000000000000000${c3}::::::::::::::::::${c1} 
  =++++++${c2}0000000000000000000${c3}:::::::::::::::::${c1}
  =+++++++${c2}000000000000000000000${c3}::::::::::::::${c1}
  =++++++++${c2}0000000000000000000000${c3}::::::::::::${c1}
  =++++++++++${c2}0000000000000000000000${c3}::::::::::${c1}
  =++++++++++++${c2}0000000000000000000000${c3}::::::::${c1}
  =++++++++++++++${c2}0000000000000000000000${c3}::::::${c1}
  -++++++++++++++++${c2}00000000000000000000${c3}::::::${c1}
   ++++++++++++++++++${c2}000000000000000000${c3}::::::${c1}
    -++++++++++++++++++-${c2}00000000000000${c3}:::::::${c1}
      `"++++++++++++++++++${c2}0000000000${c3}:::::::::
                           ":::::::::::::::::
                             ":::::::::::::: 
                               ":::::::::::  
                                 "::::::"   
EOF
	;;